{
  "name": "react-pdf",
  "version": "1.7.0",
  "description": "Easily display PDF files in your React application.",
  "main": "build/react-pdf.js",
  "es6": "src/react-pdf.jsx",
  "scripts": {
    "build": "npm run build-source && npm run build-sample-only && npm run build-test-only",
    "build-source": "webpack --progress --json > stats.source.json",
    "build-sample": "npm run build-source && npm run build-sample-only",
    "build-sample-only": "webpack --progress --config webpack.config.sample.js --json > stats.sample.json",
    "build-test": "npm run build-source && npm run build-test-only",
    "build-test-only": "webpack --progress --config webpack.config.test.js --json > stats.test.json",
    "eslint": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "npm run eslint"
  },
  "keywords": [
    "pdf",
    "pdf-viewer",
    "react"
  ],
  "author": {
    "name": "Wojciech Maj",
    "email": "kontakt@wojtekmaj.pl"
  },
  "contributors": [
    {
      "name": "Niklas NÃ¤rhinen",
      "email": "niklas@narhinen.net"
    },
    {
      "name": "Bart Van Houtte",
      "email": "bart.van.houtte@ading.be"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "pdfjs-dist": "^1.8.529",
    "prop-types": ">=15.5",
    "react": ">=15.5",
    "react-dom": ">=15.5"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "latest",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "latest",
    "less": "^2.7.2",
    "less-loader": "latest",
    "raw-loader": "^0.5.1",
    "style-loader": "latest",
    "url-loader": "latest",
    "webpack": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wojtekmaj/react-pdf.git"
  }
}
